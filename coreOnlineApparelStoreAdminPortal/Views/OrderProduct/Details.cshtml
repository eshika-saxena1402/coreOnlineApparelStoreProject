@model coreOnlineApparelStoreAdminPortal.Models.OrderProduct
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Details of Order Product</h2>
<div class="row">
    <div class="col-lg-4">
        &nbsp;
    </div>
    <div class="col-lg-4">

        <div class="form-group">
            <label for="OrderId">OrderId</label>
            <div class="form-control">
                @Html.DisplayFor(m => m.OrderId)
            </div>

            <br />
            <label for="ProductId">Productid</label>
            <div class="form-control">
                @Html.DisplayFor(m => m.ProductId)
            </div>
            @*<input type="text" class="form-control" asp-for="BrandName" />*@
        </div>
        <div class="form-group">
            <label for="Quantity">Quantity</label>
            <div class="form-control">
                @Html.DisplayFor(m => m.Quantity)
            </div>
            @*<input type="text" class="form-control" asp-for="BrandDescription" />*@
        </div>

        <a asp-controller="OrderProduct" asp-action="Index"><button type="submit" class="btn btn-primary"> BackToList</button></a>

    </div>
    <div class="col-lg-4">
        &nbsp;
    </div>
</div>


