@model coreOnlineApparelStoreAdminPortal.Models.FeedBack
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Details of FeedBack</h2>
<div class="row">
    <div class="col-lg-4">
        &nbsp;
    </div>
    <div class="col-lg-4">

        <div class="form-group">
            <label for="FeedBackId">FeedbackId</label>
            <div class="form-control">
                @Html.DisplayFor(m => m.FeedBackId)
            </div>

            <br />
            <label for="Message">message</label>
            <div class="form-control">
                @Html.DisplayFor(m => m.Message)
            </div>
            @*<input type="text" class="form-control" asp-for="BrandName" />*@
        </div>
        <div class="form-group">
            <label for="CustomerId">CustomerId</label>
            <div class="form-control">
                @Html.DisplayFor(m => m.CustomerId)
            </div>
            @*<input type="text" class="form-control" asp-for="BrandDescription" />*@
        </div>
        <a asp-controller="Feedback" asp-action="Index"><button type="submit" class="btn btn-primary"> BackToList</button></a>

    </div>
    <div class="col-lg-4">
        &nbsp;
    </div>
</div>


